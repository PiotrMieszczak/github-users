{
  "name": "github-users",
  "version": "0.0.0",
  "authors": [
    "Piotr Mieszczak <mieszczak.piotr@gmail.com>"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "lint": "ng lint --fix",
    "prepare": "husky install",
    "prettier:check": "prettier --config .prettierrc --write  --check .",
    "postinstall": "ngcc --async",
    "commit": "npx git-cz"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{ts,js}": "eslint --fix",
    "**/*.{css,html}": "npm run prettier:check"
  },
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@datorama/akita": "^7.1.1",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@taiga-ui/addon-table": "^2.45.0",
    "@taiga-ui/cdk": "^2.43.0",
    "@taiga-ui/core": "^2.43.0",
    "@taiga-ui/icons": "^2.43.0",
    "@taiga-ui/kit": "^2.43.0",
    "jest-junit": "^13.2.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.0",
    "@ngneat/spectator": "^10.0.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-marbles": "^3.0.1",
    "jest-preset-angular": "^12.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.0",
    "typescript": "~4.4.3"
  }
